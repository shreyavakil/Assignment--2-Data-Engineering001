def average(array):
    # your code goes here
    unique_values = set(array)
    avg = sum(unique_values) / len(unique_values)
    
    # Return the result rounded to 3 decimal places
    return round(avg, 3)


if name == 'main':
    n = int(input())
    arr = list(map(int, input().split()))
    result = average(arr)
    print(result)
